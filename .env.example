# This environment variable is used in Production and Preview environments to deploy your Convex project
# This variable is not needed in development.
# You can create a Production or Preview deploy key at https://dashboard.convex.dev/project/settings#production-deploy-keys
CONVEX_DEPLOY_KEY=

# Optional self-hosted Convex (leave blank for cloud)
CONVEX_SELF_HOSTED_URL=
CONVEX_SELF_HOSTED_HTTP_URL=
CONVEX_SELF_HOSTED_ADMIN_KEY=
CONVEX_DASHBOARD_URL=

# Exposed to the browser for OAuth callback URLs (auto-set by setup script)
NEXT_PUBLIC_CONVEX_HTTP_URL=

# Optional Resend configuration for password reset emails
RESEND_API_KEY=
AUTH_EMAIL_FROM=
AUTH_EMAIL_REPLY_TO=

# GitHub OAuth
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

# Google OAuth
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=
